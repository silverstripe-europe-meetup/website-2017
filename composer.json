{
	"name": "silverstripe-europe-meetup/website-2017",
	"description": "Website for StripeCon 2017 in Malta",
	"require": {
		"php": ">=7.0.0",
		"silverstripe/cms": "^3.6.0",
		"silverstripe/framework": "^3.6.0",
		"silverstripe/siteconfig": "^3.6.0",
        "silverstripe/googlesitemaps": "^1.6.0"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable",
        "zauberfisch/silverstripe-better-i18n": "dev-master",
		"lekoala/silverstripe-debugbar": "^1.0",
        "axyr/silverstripe-ideannotator": "^2.0.7",
		"deployer/deployer": "^5.1.3"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.6.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
    "scripts": {
        "dep": "dep",
        "deploy": "dep deploy",
        "ssh": "dep ssh < /dev/tty"
    },
	"prefer-stable": true,
	"minimum-stability": "dev"
}
